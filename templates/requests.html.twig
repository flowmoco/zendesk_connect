<a href="{{ path('zendesk_connect.request.new', { destination: path('zendesk_connect.requests') }) }}"><button class="" type="button" name="button">Create a ticket</button></a>
<br>
<ul class="requests-list">
    {% for request in requests.requests %}
      <a href="{{ path('zendesk_connect.request', { id: request.id }) }}">
        <li>{{ request.subject|e }}</li>
      </a>
    {% endfor %}
</ul>
